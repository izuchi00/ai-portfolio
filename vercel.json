{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "rewrites": [
    { "source": "/api/py/(.*)", "destination": "https://USERNAME-SPACENAME.hf.space/$1" },
    { "source": "/((?!api).*)", "destination": "/index.html" }
  ],
  "functions": {
    "api/ai-chat.ts": { "runtime": "nodejs20.x" },
    "api/data-analysis.ts": { "runtime": "nodejs20.x" }
  }
}